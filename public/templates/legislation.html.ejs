<div class="row">
  <div class="col-md-6 col-sm-9 col-xs-12">
    <header>
      <h1 class="module-title">
        <%= model.title %>
      </h1>
      <p class="module-description">
        <%= model.description %>
      </p>
    </header>
  </div>
</div>

<div class="row">
  <div class="col-xs-12">
    <figure>
      <div class="table-responsive">
        <table class="table">
          <thead>
            <tr>
            <% _.each(model.table.headers, function(header) { %>
              <td><%= header %></td>
            <% }) %>
            </tr>
          </thead>
          <tbody>
            <% _.each(model.table.rows, function(row) { %>
              <tr>
                <% _.each(model.table.headers, function(header, idx) { %>
                <td>
                  <% if (idx === 0) { %>
                    <%= row[header] %>
                  <% } else if (row[header]) { %>
                    <div class="dot"></div>
                  <% } %>
                </td>
                <% }) %>
              </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </figure>
  </div>
</div>
